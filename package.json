    "@babel/plugin-transform-async-to-generator": "*",
    "@babel/plugin-transform-react-inline-elements": "*",
    "@babel/plugin-transform-runtime": "*",
    "@babel/preset-react": "*",
    "@babel/register": "*",
    "@babel/runtime": "*",
